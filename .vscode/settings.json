{
  "typescript.check.workspaceVersion": false,
  "workbench.colorCustomizations": {
    "activityBar.background": "#1a755ecb"
  },
  "editor.rulers": [
    100,
    120
  ],
  "html.format.wrapAttributes": "force-aligned",
  "workbench.colorTheme": "Default Light+",
  "files.exclude": {
    "webpack.backend-bundle-build.js": true,
    "node_modules": true,
    "tnp-system-path.txt": true,
    "tmp*": true,
    "dist*": true,
    "bundle*": true,
    "browser": true,
    "browser*": true,
    "module*": true,
    "backup": true,
    "module": true,
    "www": true,
    "Thumbs.db": true,
    ".DS_Store": true,
    "**/.DS_Store": true,
    "npm-debug.log*": true,
    ".sass-cache": true,
    ".sourcemaps": true,
    "tsconfig.json": true,
    "tsconfig.isomorphic.json": true,
    "tsconfig.browser.json": true,
    ".npmrc": true,
    "tslint.json": true,
    ".editorconfig": true,
    "index.js": true,
    "index.d.ts": true,
    "index.js.map": true,
    ".vscode/tasks.json": true,
    ".vscode/launch.json": true,
    "webpack.config.js": true,
    "run.js": true,
    "tsconfig.json.filetemplate": true,
    "tsconfig.isomorphic.json.filetemplate": true,
    "tsconfig.browser.json.filetemplate": true,
    ".gitignore": true,
    ".npmignore": true,
    ".babelrc": true,
    "docs": true,
    "logo.svg": true,
    "bin/db.json": false
  }
}